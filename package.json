{
  "name": "@krassowski/jupyterlab-lsp",
  "version": "0.5.0",
  "description": "Language Server Protocol integration for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "lsp",
    "language-server-protocol",
    "intellisense",
    "code-intelligence",
    "linting"
  ],
  "homepage": "https://github.com/krassowski/jupyterlab-lsp",
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "bugs": {
    "url": "https://github.com/krassowski/jupyterlab-lsp/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Micha≈Ç Krassowski",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/krassowski/jupyterlab-lsp.git"
  },
  "scripts": {
    "bootstrap": "jlpm && jlpm clean && jlpm lint && jlpm build",
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "lint": "jlpm prettier && jlpm tslint",
    "lint:check": "jlpm prettier:check && jlpm tslint:check",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md,.yml}'",
    "prettier:check": "prettier --list-different '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md,.yml}'",
    "tslint": "tslint --fix -c tslint.json --project tsconfig.json '**/*{.ts,.tsx}'",
    "tslint:check": "tslint -c tslint.json --project tsconfig.json '**/*{.ts,.tsx}'",
    "test": "jlpm lint:check && jlpm test:jest",
    "test:jest": "jlpm jest --coverage --coverageReporters=cobertura --coverageReporters=html --coverageReporters=text-summary"
  },
  "dependencies": {
    "lsp-editor-adapter": "0.0.10"
  },
  "devDependencies": {
    "@krassowski/jupyterlab_go_to_definition": "file:vendor/jupyterlab-go-to-definition",
    "@babel/preset-env": "^7.4.3",
    "@jupyterlab/application": "~1.2.0-rc.0",
    "@jupyterlab/apputils": "~1.2.0-rc.0",
    "@jupyterlab/cells": "~1.2.0-rc.0",
    "@jupyterlab/codeeditor": "~1.2.0-rc.0",
    "@jupyterlab/codemirror": "~1.2.0-rc.0",
    "@jupyterlab/completer": "~1.2.0-rc.0",
    "@jupyterlab/coreutils": "~3.2.0-rc.0",
    "@jupyterlab/docmanager": "~1.2.0-rc.0",
    "@jupyterlab/docregistry": "~1.2.0-rc.0",
    "@jupyterlab/fileeditor": "~1.2.0-rc.0",
    "@jupyterlab/notebook": "~1.2.0-rc.0",
    "@jupyterlab/rendermime": "~1.2.0-rc.0",
    "@jupyterlab/services": "~4.2.0-rc.0",
    "@jupyterlab/statusbar": "~1.2.0-rc.0",
    "@jupyterlab/testutils": "^1.0.0-alpha.6",
    "@jupyterlab/tooltip": "~1.2.0-rc.0",
    "@phosphor/algorithm": "^1.2.0",
    "@types/chai": "^4.1.7",
    "@types/codemirror": "^0.0.74",
    "@types/events": "^3.0.0",
    "@types/jest": "^23.3.11",
    "bash-language-server": "^1.6.1",
    "chai": "^4.2.0",
    "codemirror": "~5.47.0",
    "dockerfile-language-server-nodejs": "^0.0.21",
    "events": "^3.0.0",
    "husky": "^3.0.9",
    "javascript-typescript-langserver": "^2.11.3",
    "jest": "^24.7.1",
    "jest-junit": "^8.0.0",
    "jsonrpc-ws-proxy": "0.0.5",
    "precise-commits": "^1.0.2",
    "prettier": "^1.18.2",
    "react": "~16.8.4",
    "rimraf": "^2.6.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "~3.5.1",
    "unified-language-server": "^0.3.0",
    "vscode-css-languageserver-bin": "^1.4.0",
    "vscode-html-languageserver-bin": "^1.4.0",
    "vscode-json-languageserver-bin": "^1.0.1",
    "yaml-language-server": "^0.4.0"
  },
  "peerDependencies": {
    "@krassowski/jupyterlab_go_to_definition": "file:vendor/jupyterlab-go-to-definition",
    "@jupyterlab/application": "~1.2.0-rc.0",
    "@jupyterlab/apputils": "~1.2.0-rc.0",
    "@jupyterlab/cells": "~1.2.0-rc.0",
    "@jupyterlab/codeeditor": "~1.2.0-rc.0",
    "@jupyterlab/codemirror": "~1.2.0-rc.0",
    "@jupyterlab/completer": "~1.2.0-rc.0",
    "@jupyterlab/coreutils": "~3.2.0-rc.0",
    "@jupyterlab/docmanager": "~1.2.0-rc.0",
    "@jupyterlab/docregistry": "~1.2.0-rc.0",
    "@jupyterlab/fileeditor": "~1.2.0-rc.0",
    "@jupyterlab/notebook": "~1.2.0-rc.0",
    "@jupyterlab/rendermime": "~1.2.0-rc.0",
    "@jupyterlab/services": "~4.2.0-rc.0",
    "@jupyterlab/statusbar": "~1.2.0-rc.0",
    "@jupyterlab/testutils": "^1.0.0-alpha.6",
    "@jupyterlab/tooltip": "~1.2.0-rc.0",
    "@phosphor/algorithm": "^1.2.0",
    "codemirror": "~5.47.0",
    "react": "~16.8.4"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "base": {
          "name": "jupyter-lsp"
        },
        "managers": [
          "pip"
        ]
      }
    },
    "schemaDir": "schema"
  }
}
