{
  "name": "@krassowski/jupyterlab-symbol-tree",
  "version": "0.7.0",
  "description": "Hierarchical symbol viewer for Language Servers in JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "lsp",
    "language-server-protocol"
  ],
  "homepage": "https://github.com/krassowski/jupyterlab-lsp",
  "bugs": {
    "url": "https://github.com/krassowski/jupyterlab-lsp/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Micha≈Ç Krassowski",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/krassowski/jupyterlab-lsp.git"
  },
  "scripts": {
    "build": "tsc -b",
    "bundle": "npm pack .",
