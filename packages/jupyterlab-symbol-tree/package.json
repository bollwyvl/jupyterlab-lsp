  },
  "dependencies": {
    "@krassowski/jupyterlab-lsp": "^0.7.0"
  },
  "devDependencies": {
    "@jupyterlab/application": "~1.1.3",
    "react": "~16.8.4",
    "rimraf": "~2.6.2",
    "typescript": "~3.5.1"
  },
  "peerDependencies": {
    "@jupyterlab/application": "~1.1.3",
    "react": "~16.8.4"
  },
  "jupyterlab": {
    "extension": "lib/plugin.js",
    "schemaDir": "schema"
  }
}
